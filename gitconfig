[color]
	branch = auto
	ui = true
[color "branch"]
	current = yellow bold
	local = white
	remote = yellow dim
[color "diff"]
	commit = yellow bold
	frag = magenta bold
	meta = yellow
	new = green bold
	old = red bold
	whitespace = red reverse
[color "status"]
	added = cyan bold
	branch = yellow bold
	changed = red bold
	untracked = magenta bold
[alias]
	br = branch
	cm = commit
	co = checkout
	cr = checkout -b
	df = diff
	ds = diff --staged
	fe = fetch -tpP
	last = log --oneline -n 1
	minlog = log --oneline
	graph = log --color --graph --oneline
	pn = push origin -u HEAD
	pu = push
	pl = pull
	up = !git fetch -tpP && git pull
	st = !git branch && git status
	statlog = log --stat
	taglist = tag -l --sort=-v:refname
	tagl = tag -l --sort=-v:refname
[credential]
	helper = osxkeychain
[push]
	default = current
[user]
	name = Edward Seim
	email = sirseim@gmail.com
[core]
	excludesfile = /Users/sirseim/.gitignore_global
[pull]
	rebase = false
[url "git@github.com:"]
	insteadOf = https://github.com/
